<p id="notice"><%= notice %></p>

  <tbody>
    <% @titles.each do |title| %>
      <tr>
        <p>
          <%= image_tag(title.picture_url, class: "img-rounded img-responsive") if title.picture.present? %>
          <% hoge = title.author %>
          <%= hoge.html_safe %>
        </p>
      </tr>
    <% end %>
  </tbody>
</table>
